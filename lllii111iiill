local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "灾难蛋的脚本(内测版本)|Stands awakening", HidePremium = false, SaveConfig = true, ConfigFolder = "SAconfig"})
local TeleportService = game:GetService('TeleportService')
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer







--Values
_G.AutoTimeStop = true
_G.AutoBlock = true
_G.InfJump = true
_G.AutoRoka = true
_G.AutoArrow = true
_G.InfDamage = true
_G.AutoClone = true
_G.Menacing = true
_G.AutoD4c = true
_G.AutoDodge = true
_G.LeftArmStandTrails = true
_G.RightArmStandTrails = true







--Functions

function AutoTimeStop()
while _G.AutoTimeStop == true do
    local args = {
        [1] = 114514,
        [2] = "thwoh"
    }
    
    game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args))
    wait(1)
    end
end

function AutoBlock()
    while _G.AutoBlock == true do
        local args = {
            [1] = "Alternate",
            [2] = "Block"
        }
        
        game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
        wait(0.5)
    end
end



function AutoRoka()
   while _G.AutoRoka == true do
   game:GetService("ReplicatedStorage").ItemEvents.Roka:FireServer()
wait(0.3)
end
end

function AutoArrow()
while _G.AutoArrow == true do
game:GetService("ReplicatedStorage").ItemEvents.Arrow:FireServer()
wait(0.3)
end
end

function InfDamage()
while _G.InfDamage == true do
local args = {
    [1] = "Alternate",
    [2] = "RTZ",
    [3] = true
}

game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
wait(2)
end
end

function AutoClone()
while _G.AutoClone == true do
local args = {
    [1] = "Alternate",
    [2] = "Clone"
}

game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
wait(0.1)
end
end

function Menacing()
while _G.Menacing == true do
local args = {
    [1] = true
}

game:GetService("ReplicatedStorage").Main.Menacing:FireServer(unpack(args))
wait(20)
end
end

function AutoD4c()
while _G.AutoD4c == true do
local args = {
    [1] = "Alternate",
    [2] = "Deflect"
}

game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
wait(5)
end
end

function AutoDodge()
while _G.AutoDodge == true do
local args = {
    [1] = "Alternate",
    [2] = "STWRTZ",
    [3] = true
}

game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
wait(7)
end
end

function LeftArmStandTrails()
	while _G.LeftArmStandTrails == true do
		local args = {
			[1] = game:GetService("Players").LocalPlayer.Character.Stand["Stand Left Arm"].Trail,
			[2] = true
		}
		
		game:GetService("ReplicatedStorage").Trail:FireServer(unpack(args))
		wait(0.3)
	end
end

function RightArmStandTrails()
	while _G.RightArmStandTrails == true do
		local args = {
			[1] = game:GetService("Players").LocalPlayer.Character.Stand["Stand Right Arm"].Trail,
			[2] = true
		}
		
		game:GetService("ReplicatedStorage").Trail:FireServer(unpack(args))
		wait(0.3)
	end
end










--Tabs
local FirstTab = Window:MakeTab({
	Name = "功能类",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = FirstTab:AddSection({
	Name = "时停类角色需要"
})
FirstTab:AddButton({
	Name = "时停",
	Callback = function()
     local args = {
    [1] = 114514,
    [2] = "thwoh"
}

game:GetService("ReplicatedStorage").Main.Timestop:FireServer(unpack(args)) 		
  	end    
})


FirstTab:AddToggle({
	Name = "自动时停",
	Default = false,
	Callback = function(Value)
		_G.AutoTimeStop = Value
        AutoTimeStop()
	end    
})

local Section = FirstTab:AddSection({
	Name = "需要黄镇类"
})

FirstTab:AddToggle({
	Name = "无限反伤",
	Default = false,
	Callback = function(Value)
		_G.InfDamage = Value
        InfDamage()
	end    
})


